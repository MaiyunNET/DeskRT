<form v-model:width="width" v-model:height="height" title="Test program" icon="/res/icon.svg">
    <overflow flex="1">
        <layout class="layout" direction="v" gutter="10">
            <label>focus: {{focus}}</label>
            <button :disabled="true">Disabled button</button>
            <button @tap="openOnlyClose">Open only close button form</button>
            <button @tap="openThin">Open thin form</button>
            <button @tap="openBorderNone">Open border none form</button>
            <button @tap="openAero">Open Aero form (CSS)</button>
            <button @tap="openMax">Open default max form</button>
            <button @tap="openMove">Open test bindMove form</button>
            <button @tap="openScroll">Open ScrollBar form</button>
            <button @tap="openView">Open View form</button>
            <button @tap="openMenu">Open menu form</button>
            <button @tap="openGreatView">Open GreatView &amp; Select form</button>
            <button @tap="openOverflow">Open Overflow form</button>
            <button @tap="runTaskmgr">Run "Task manager" app</button>
            <layout gutter="10">
                <button @tap="changeTheme" flex="1">Theme</button>
                <button @tap="openError" flex="1">Error</button>
            </layout>
        </layout>
    </overflow>
</form>